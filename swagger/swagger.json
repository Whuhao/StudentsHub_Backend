                    "format": "int64"
                }
            }
        },
        "models.User": {
            "title": "User",
            "type": "object",
            "properties": {
                "CreateAt": {
                    "type": "string",
                    "format": "datetime"
                },
                "Password": {
                    "type": "string"
                },
                "Role": {
                    "type": "string"
                },
                "UpdateAt": {
                    "type": "string",
                    "format": "datetime"
                },
                "UserID": {
                    "type": "integer",
                    "format": "int64"
                },
                "UserName": {
                    "type": "string"
                }
            }
        }
